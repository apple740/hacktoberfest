#include <stdio.h>
#include <string.h>

int main() {

    char s[1000];int i,j;
   gets(s);
   int n=strlen(s);
    for (i=0;i<n;i++)
    {
        if (s[i]==' ')
        {
           for(j=0;j<i;j++)
           {
               printf("%c\n",s[j]);
               j=i+1;
           }
        }
    }
    return 0;
}
