#include<stdio.h>
int main()
{
 int sum,sum1,k,n,m;
 int a[n][n];
 scanf("%d",&n);
 for(int i=0;i<n;i++)
 {
  for(int j=0;j<n;j++)
  {
      scanf("%d",&a[i][j]);
  }
 }
  for(int i=0;i<n;i++)
 {
  for(int j=0;j<m;j++)
  {
     if(i==j){
         sum=sum+a[i][j];
     } 
  }
  for(int j=0;j<m;j++)
  {
     for(int k=n;k>0;k--)
     {
         sum1=sum1+a[i][k];
     }
     } 
  }
   
   sum=sum-sum1;
   printf("%d",sum);
 }

 


